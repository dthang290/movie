{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@routers/*": [
        "src/routers/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@templates/*": [
        "src/templates/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
    },
  },
}
